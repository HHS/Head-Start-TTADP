    "@opensearch-project/opensearch": "^1.1.0",
    "@ttahub/common": "2.0.11",
    "adm-zip": "^0.5.1",
    "aws-sdk": "^2.826.0",
    "aws4": "^1.11.0",
    "axios": "^0.24.0",
    "bull": "^4.2.0",
    "cheerio": "^1.0.0-rc.10",
    "client-oauth2": "^4.3.3",
    "cls-hooked": "^4.2.2",
